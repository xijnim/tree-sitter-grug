{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "grug",
      "camelcase": "Grug",
      "title": "Grug",
      "scope": "source.grug",
      "file-types": [
        "grug"
      ],
      "injection-regex": "^grug$",
      "class-name": "TreeSitterGrug",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A tree sitter parser for grug",
    "authors": [
      {
        "name": "xijnim",
        "email": "xijnin@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/xijnim/tree-sitter-grug.git"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
